generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ProjectImage {
  id        String   @id @default(cuid())
  src       String
  alt       String
  order     Int
  category  String   @default("private") // healthcare, education, government, private
  publicId  String?
  createdAt DateTime @default(now())
}
